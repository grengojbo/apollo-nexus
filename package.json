{
	"name": "apollo-nexus",
	"version": "1.0.0",
	"main": "index.js",
	"license": "MIT",
	"private": true,
	"description": "Example Apollo Server nexes shema generation",
	"scripts": {
		"start": "node dist/server",
		"clean": "rm -rf dist",
		"build": "npm -s run clean && npm -s run generate && tsc",
		"generate": "yarn -s generate:types && yarn -s generate:schema && yarn -s generate:prisma && yarn -s generate:nexus",
		"generate:types": "create-types",
		"generate:schema": "cnt --mq -f -o",
		"generate:prisma": "prisma generate",
		"generate:nexus": "ts-node --transpile-only src/nexusSchema",
		"db:create": "prisma migrate up --experimental --auto-approve --create-db",
		"migration:save": "prisma migrate save --experimental",
		"migration:up": "prisma migrate up --experimental --auto-approve",
		"migration:down": "prisma migrate down 1 --experimental",
		"dev": "ts-node-dev --no-notify --respawn --transpile-only src/server",
		"seed": "ts-node prisma/seed.ts",
		"style": "prettier --write 'src/**/*'",
		"lint": "eslint ."
	},
	"dependencies": {
		"@nexus/schema": "0.16.0",
		"@paljs/nexus": "2.2.1",
		"@prisma/client": "2.8.1",
		"apollo-server": "2.18.2",
		"apollo-server-fastify": "^3.0.0-alpha.3",
		"dotenv": "^8.2.0",
		"fastify": "^3.5.1",
		"fastify-favicon": "^3.0.0",
		"fastify-healthcheck": "^3.0.0",
		"fastify-language-parser": "^3.0.0",
		"fastify-metrics": "^6.0.2",
		"graphql": "15.3.0",
		"nexus-plugin-prisma": "^0.21.0",
		"pino-pretty": "^4.3.0",
		"yn": "^4.0.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^11.0.0",
		"@commitlint/config-conventional": "^11.0.0",
		"@prisma/cli": "2.8.1",
		"@types/faker": "^5.1.2",
		"@types/jest": "^26.0.14",
		"@types/node": "14.11.8",
		"@typescript-eslint/eslint-plugin": "^4.4.0",
		"@typescript-eslint/parser": "^4.4.0",
		"codecov": "^3.8.0",
		"conventional-changelog-cli": "^2.1.0",
		"create-nexus-type": "^1.3.0",
		"eslint": "^7.11.0",
		"eslint-config-google": "^0.14.0",
		"eslint-config-prettier": "^6.12.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-jest": "^24.1.0",
		"eslint-plugin-prettier": "^3.1.4",
		"eslint-plugin-sort-requires": "^2.1.0",
		"faker": "^5.1.0",
		"husky": "^4.3.0",
		"jest": "^26.5.2",
		"prettier": "2.1.2",
		"prettier-plugin-packagejson": "^2.2.6",
		"pretty-quick": "^3.0.2",
		"replace": "^1.2.0",
		"ts-jest": "^26.4.1",
		"ts-node": "8.10.2",
		"ts-node-dev": "1.0.0-pre.63",
		"typedoc": "^0.19.2",
		"typedoc-plugin-markdown": "^3.0.8",
		"typescript": "4.0.3"
	},
	"peerDependencies": {
		"graphql": "^0.12.0 || ^0.13.0 || ^14.0.0 || ^15.0.0"
	},
	"engines": {
		"node": ">=12"
	},
	"repository": "https://github.com/grengojbo/apollo-nexus.git",
	"author": "Oleg Dolya<oleg.dolya@gmail.com>"
}
